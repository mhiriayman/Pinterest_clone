<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Pinterest{% endblock %}</title>
    {% block stylesheets %}
       {{ encore_entry_link_tags('app') }}
    {% endblock %}

</head>
<body>
{% for type, messages in app.flashes(['success','error','info']) %}
    {% for message in messages %}
        <div role="alert" class="alert alert-{{ type }}">{{ message }}</div>
    {% endfor %}
{% endfor %}

{{ include('home/_nav.html.twig') }}
<main role="main" class="container">
    {% block body %}

    {% endblock %}
</main>
{{ include('home/_footer.html.twig') }}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
